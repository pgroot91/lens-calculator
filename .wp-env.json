{
  "core": "https://wordpress.org/wordpress-6.8.2.zip",
  "env": {
    "development": {
      "plugins": ["."],
      "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "WP_DEBUG_DISPLAY": true,
        "SCRIPT_DEBUG": true,
        "WP_MEMORY_LIMIT": "256M",
        "WP_MAX_MEMORY_LIMIT": "512M"
      }
    },
    "tests": {
      "plugins": ["."],
      "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "WP_DEBUG_DISPLAY": false,
        "SCRIPT_DEBUG": true,
        "WP_MEMORY_LIMIT": "256M",
        "WP_MAX_MEMORY_LIMIT": "512M"
      }
    }
  }
}
